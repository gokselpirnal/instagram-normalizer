{
  "manifest_version": 2,
  "name": "instagram-normalizer",
  "description": "Instagram video controls and copy image or video url",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["instagram.js"]
    }
  ],
  "permissions": [
    "*://*.instagram.com/*"
  ]
}
